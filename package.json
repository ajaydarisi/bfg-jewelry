  "private": true,
  "scripts": {
    "dev": "next dev --hostname 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "cap:sync": "cap sync",
    "cap:android": "cap sync android && cap open android",
    "cap:ios": "cap sync ios && cap open ios",
    "cap:clean": "rm -rf android/app/build android/build android/.gradle",
    "cap:install": "npm run cap:clean && cap sync android && cd android && JAVA_HOME=/usr/lib/jvm/java-21-openjdk-amd64 ./gradlew assembleDebug && cd .. && adb install -r android/app/build/outputs/apk/debug/app-debug.apk",
    "cap:dev": "cap sync android && cap run android -l --host $(hostname -I | awk '{print $1}') --port 3000"
  },
